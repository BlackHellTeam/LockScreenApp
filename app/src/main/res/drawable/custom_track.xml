<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:visible="true"
    android:dither="true"
    android:useLevel="false">

    <gradient
        android:startColor="#989a9d"
        android:endColor="#c3c5c7"
        android:angle="270"/>

    <corners
        android:radius="25dp"/>

    <size
        android:width="200dp"
        android:height="50dp" />

    <stroke
        android:width="4dp"
        android:color="#707173"/>

</shape>